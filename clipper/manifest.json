{
    "manifest_version": 2,
    "name": "lanting-clipper",
    "version": "0.1.0",
    "description": "",
    "homepage_url": "https://lanting.wiki",
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "icons": {
        "32": "icons/icon.png",
        "48": "icons/icon.png",
        "96": "icons/icon.png"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "http://*/",
        "https://*/",
        "<all_urls>",
        "*://*/*",
        "storage"
    ],
    "browser_action": {
        "default_icon": "icons/icon.png",
        "default_title": "lanting-clipper",
        "default_popup": "popup/build/index.html"
    },
    "commands": {
        "_execute_browser_action": {
        },
        "clipCompletePage": {
            "description": "Clip complete page (uses last selected notebook)"
        },
        "clipCompletePageHtml": {
            "suggested_key": {
                "default": "Ctrl+1",
                "mac": "MacCtrl+1"
            },
            "description": "Clip complete page (HTML) (uses last selected notebook)"
        },
        "clipSimplifiedPage": {
            "description": "Clip simplified page (uses last selected notebook)"
        },
        "clipUrl": {
            "description": "Clip url (uses last selected notebook)"
        },
        "clipSelection": {
            "description": "Clip selection (uses last selected notebook)"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    }
}